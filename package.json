{
  "name": "jss-react-app",
  "version": "1.0.0",
  "description": "A starter kit for a React app w/ Babel, TypeScript, Webpack, and tools.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "prettier": "prettier \"src/**/*.+(js|jsx|ts|tsx|json|css|scss)\"",
    "pretty-check": "npm run prettier -- --check",
    "prettify": "npm run prettier -- --write",
    "lint": "eslint src --ext=js --ext=jsx --ext=ts --ext=tsx",
    "validate": "tsc && lint-staged",
    "test": "echo \"No test specified\""
  },
  "author": "Jeremy Sabath",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "1.16.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-copyfiles-plugin": "^1.0.6",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime": "^7.3.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate && npm run test"
    }
  }
}
