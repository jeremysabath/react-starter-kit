{
  "presets": [
    [
      "@babel/preset-env",
      { "useBuiltIns": "entry", "corejs": { "version": 2 } }
    ],
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    "babel-plugin-styled-components"
  ]
}
